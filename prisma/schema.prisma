generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DB_URL")
}

model User {
    userId   String    @id @db.VarChar(50)
    password String    @db.VarChar(100)
    email    String    @unique @db.VarChar(100)
    nickname String    @unique @db.VarChar(10)
    birthday DateTime? @db.Date
}
