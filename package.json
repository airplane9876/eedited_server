{
    "name": "eedited_rest_api_server",
    "version": "0.1.0",
    "description": "SW Maestro 12th Team.eedited's Rest API Server",
    "main": "index.js",
    "engines": {
        "node": "14.16.1"
    },
    "engineStrict": true,
    "scripts": {
        "start": "npm run db:cligen && npm run build:force && cross-env NODE_ENV=production ./node_modules/.bin/pm2 start ./dist -i -0",
        "shutdown": "./node_modules/.bin/pm2 kill",
        "reload": "./node_modules/.bin/pm2 reload all",
        "ls": "./node_modules/.bin/pm2 list",
        "log": "./node_modules/.bin/pm2 logs",
        "log:err": "./node_modules/.bin/pm2 logs - err",
        "monitor": "./node_modules/.bin/pm2 monit",
        "build": "./node_modules/.bin/tsc --build",
        "build:force": "./node_modules/.bin/tsc --build --force",
        "clean": "./node_modules/.bin/tsc --build --clean",
        "dev": "cross-env NODE_ENV=development ./node_modules/.bin/tsc-watch --noEmit --onSuccess \"ts-node --files ./src\"",
        "dev:src": "npm run dev",
        "dev:dist": "cross-env NODE_ENV=development ./node_modules/.bin/tsc-watch --onSuccess \"node ./dist\"",
        "dev:server": "npm run db:cligen && npm run build:force && cross-env NODE_ENV=development ./node_modules/.bin/pm2 start ./dist -i -0",
        "db:push": "./node_modules/.bin/prisma db push",
        "db:create": "npm run db:push && npm run db:cligen",
        "db:reverse": "npm run db:introspect && npm run db:cligen",
        "db:cligen": "./node_modules/.bin/prisma generate",
        "db:introspect": "./node_modules/.bin/prisma introspect",
        "db:migrate": "./node_modules/.bin/prisma migrate",
        "db:show": "./node_modules/.bin/prisma studio",
        "test": "./node_modules/.bin/jest",
        "test:cover": "./node_modules/.bin/jest --coverage"
    },
    "author": "eedited",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^2.27.0",
        "aws-sdk": "^2.954.0",
        "bcrypt": "^5.0.1",
        "chalk": "^4.1.1",
        "connect-redis": "^6.0.0",
        "cookie-parser": "^1.4.5",
        "cross-env": "^7.0.3",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "helmet": "^4.6.0",
        "hpp": "^0.2.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "multer-s3": "^2.9.0",
        "nodemailer": "^6.6.3",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "pm2": "^5.1.0",
        "redis": "^3.1.2",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/connect-redis": "0.0.17",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/hpp": "^0.2.2",
        "@types/morgan": "^1.9.3",
        "@types/multer": "^1.4.7",
        "@types/multer-s3": "^2.7.10",
        "@types/node": "^16.3.2",
        "@types/nodemailer": "^6.4.4",
        "@types/passport": "^1.0.7",
        "@types/passport-local": "^1.0.34",
        "@types/redis": "^2.8.31",
        "@types/validator": "^13.6.3",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "eslint": "^7.30.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.23.4",
        "jest": "^27.0.6",
        "prisma": "^2.27.0",
        "ts-node": "^10.1.0",
        "tsc-watch": "^4.4.0",
        "typescript": "^4.3.5"
    }
}
