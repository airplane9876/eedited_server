{
    "name": "eedited_rest_api_server",
    "version": "0.0.0",
    "description": "SW Maestro 12th Team.eedited's Rest API Server",
    "main": "index.js",
    "engines": {
        "node": "14.16.1"
    },
    "scripts": {
        "start": "NODE_ENV=production PORT=80 npm run build:force && node ./dist",
        "build": "./node_modules/.bin/tsc --build",
        "build:force": "./node_modules/.bin/tsc --build --force",
        "clean": "./node_modules/.bin/tsc --build --clean",
        "dev": "./node_modules/.bin/tsc-watch --noEmit --onSuccess \"ts-node --files ./src\"",
        "dev:dist": "./node_modules/.bin/tsc-watch --onSuccess \"node ./dist\"",
        "test": "./node_modules/.bin/jest",
        "coverage": "./node_modules/.bin/jest --coverage"
    },
    "author": "eedited",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^2.27.0",
        "bcrypt": "^5.0.1",
        "cookie-parser": "^1.4.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "morgan": "^1.10.0",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/morgan": "^1.9.3",
        "@types/node": "^16.3.2",
        "@types/passport": "^1.0.7",
        "@types/passport-local": "^1.0.34",
        "@types/validator": "^13.6.3",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "eslint": "^7.30.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.23.4",
        "jest": "^27.0.6",
        "prisma": "^2.27.0",
        "ts-node": "^10.1.0",
        "tsc-watch": "^4.4.0",
        "typescript": "^4.3.5"
    }
}
