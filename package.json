{
    "name": "eedited_rest_api_server",
    "version": "0.0.0",
    "description": "SW Maestro 12th Team.eedited's Rest API Server",
    "main": "index.js",
    "scripts": {
        "start": "NODE_ENV=production PORT=80 npm run build:force && node ./dist",
        "build": "./node_modules/.bin/tsc --build",
        "build:force": "./node_modules/.bin/tsc --build --force",
        "clean": "./node_modules/.bin/tsc --build --clean",
        "dev": "./node_modules/.bin/tsc-watch --onSuccess \"node ./dist\"",
        "test": "./node_modules/.bin/jest",
        "coverage": "./node_modules/.bin/jest --coverage"
    },
    "author": "eedited",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "cookie-parser": "^1.4.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "morgan": "^1.10.0",
        "mysql2": "^2.2.5",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "sequelize": "^6.6.5",
        "sequelize-cli": "^6.2.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^16.3.1",
        "@types/validator": "^13.6.3",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "eslint": "^7.30.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.23.4",
        "jest": "^27.0.6",
        "nodemon": "^2.0.12",
        "tsc-watch": "^4.4.0",
        "typescript": "^4.3.5"
    }
}
